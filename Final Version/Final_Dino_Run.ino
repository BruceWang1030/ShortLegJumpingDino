  tft.fillRect(0, 120, 32, 46, BLACK);
  tft.fillRect(0, 74, 32, 46, BLACK);
  tft.drawBitmap(0, 74, jumphigh, 32, 46, WHITE);
  detect();
  delay(100);
  drawObject();
  detect();
  delay(100);
  drawObject();
  detect();
  //down
  dino_low_pos = 1;
  dino_up_pos = 1;
  tft.fillRect(0, 74, 32, 46, BLACK);
  tft.fillRect(0, 120, 32, 46, BLACK);
  tft.drawBitmap(0, 74, halfhead, 32, 46, WHITE);
  tft.drawBitmap(0, 120, halfbody, 32, 46, WHITE);
  delay(100);
  tft.fillRect(0, 74, 32, 46, BLACK);
  dino_up_pos = 0;
  dino_low_pos = 1;
  detect();
}

//Generates objects at random
void objectRandom (int upperBG[], int lowerBG[]) {
  int ObjectPos = random(5, 10);
  Serial.println(ObjectPos);
  //Object Array
  switch (ObjectPos) {
    case upperObject:
      lowerBG[10] = empty;
      //upper and lower too close
      if ((lowerBG[9] == lowerObject) || (lowerBG[8] == lowerObject)) upperBG[10] = empty;
      else upperBG[10] = upperObject;
      break;
    case lowerObject:
      //Max two lower objects in a row or upper and lower too close
      if ((lowerBG[9] == lowerObject) || (lowerBG[8] == lowerObject) || (lowerBG[7] == lowerObject) || (upperBG[9] == upperObject) || (upperBG[8] == upperObject)) {
        lowerBG[10] = empty;
      }
      else lowerBG[10] = lowerObject;
      break;
    case empty:
    case 8: //empty2
    case 9: //empty3
    case 10: //empty4
      upperBG[10] = empty;
      lowerBG[10] = empty;
      break;
  }
}

//BitMap object generator 
unsigned char *objects(int n, const unsigned char top1 [] PROGMEM, const unsigned char bot1 [] PROGMEM, const unsigned char empt [] PROGMEM) {
  switch (n) {
    case 5:
      return top1;
    case 6:
      return bot1;
    case 7:
      return empt;
  }
}

//Draws object on map
void drawObject () {
  objectRandom(upperBG, lowerBG);
  for (int i = 0; i < 10; i++) {
    upperBG[i] = upperBG[i + 1];
    lowerBG[i] = lowerBG[i + 1];
  }
  if (upperBG[0] != 7) {
    tft.drawBitmap(0, 74, top1, 32, 46, WHITE);
  }
  else if (dino_up_pos == 0) {
    tft.fillRect(0, 74, 32, 46, BLACK);
  }
  if (lowerBG[0] != 7) {
    tft.drawBitmap(0, 120, bot1, 32, 46, WHITE);
  }
  else if (dino_low_pos == 0) {
    tft.fillRect(0, 120, 32, 46, BLACK);
  }
  for (int i = 1; i < 10; i++) {
    tft.fillRect(32 * i, 74, 32, 46, BLACK);
    tft.drawBitmap(32 * i, 74, objects(upperBG[i], top1, bot1, empt), 32, 46, WHITE);
    tft.fillRect(32 * i, 120, 32, 46, BLACK);
    tft.drawBitmap(32 * i, 120, objects(lowerBG[i], top1, bot1, empt), 32, 46, WHITE);
  }
}


//Collision with Object Detection
void detect() {
  if ((dino_up_pos == 1 && upperBG[0] != 7) || (dino_low_pos == 1 && lowerBG[0] != 7)) {
    tft.fillScreen(BLACK);
    playing = false;
    scoring = false;
  }
}


//Game Over Screen (when character collides with object)
void over() {
  tft.fillScreen(BLACK);
  tft.setCursor (50, 80);
  tft.setTextSize(4);
  tft.println ("GAME OVER");
  tft.setCursor (60, 140);
  tft.setTextSize(3);
  tft.println ("Score: ");
  tft.setCursor (210, 140);
  tft.println (score);
  delay(100000000000000000);
}


//Start game (When user touches screen)
void waitOneTouch(void) {
  TSPoint p;
  do {
    p = ts.getPoint();
    pinMode(XM, OUTPUT);
    pinMode(YP, OUTPUT);
  } while ((p.z < MINPRESSURE ) || (p.z > MAXPRESSURE));
  if (p.z > MINPRESSURE && p.z < MAXPRESSURE) {
    Serial.println(F("ScreenTouch detect, start"));
    playing = true;
    scoring = true;
    tft.fillScreen(BLACK);
    tft.fillRect(0, 166, 320, 5, WHITE);
  }
}

