void drawDinoJump() {
  //half up + down
  dino_low_pos = 1;
  dino_up_pos = 1;
  tft.setCursor (0, 0);
  tft.fillRect(0, 120, 32, 46, BLACK);
  tft.fillRect(0, 74, 32, 46, BLACK);
