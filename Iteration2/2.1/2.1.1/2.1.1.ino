void drawDinoWalk(){
  dino_low_pos=1;
  tft.fillRect(0, 120, 32, 46, BLACK);
  tft.drawBitmap(0, 120, run2,32, 46, WHITE);
  delay(50);
  tft.fillRect(0, 120, 32, 46, BLACK);
