  detect();
}
void drawDinoJump(){
  //half up + down
  dino_low_pos=1;
  dino_up_pos=1;
  tft.setCursor (0,0);
  tft.drawBitmap(0, 74, halfhead,32, 46, RED);
  tft.drawBitmap(0, 120, halfbody,32, 46, RED);
  detect();
  
  delay(200);
  drawObject();
  //up
  dino_low_pos=0;
  dino_up_pos=1;
  tft.drawBitmap(0, 74, jumphigh,32, 46, RED);
  tft.drawBitmap(0, 120, empt,32, 46, YELLOW);
  detect();
  
  
  delay(200);
  drawObject();
  detect();
  delay(200);
  drawObject();
  detect();
  
  //down
  dino_low_pos=1;
  dino_up_pos=1;

  tft.drawBitmap(0, 74, halfhead,32, 46, RED);
  tft.drawBitmap(0, 120, halfbody,32, 46, RED);
  delay(200);
  tft.drawBitmap(0,74, empt,32, 46, YELLOW);
  dino_up_pos=0;
  dino_low_pos=1;
  detect();
}
